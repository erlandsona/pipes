identity l \l

const a b \a

flip f a b \f b a

(x y. x) 123

: [BadRequest] Person
x y
  \ x
   \ stuff : [Int] Str
   | Err int
   \ Sum.onClick
    \ y int
     \ things : String -> [BadRequest] Person




- nat -> nat -> Bool
check one two
  \one = two

- bool -> bool -> bool
and 1 2
  \1 & 2

- bool
true t f
  t

false t f
  f

and x y t f
  x (y t f) f

or x y t f
  x t (y t f)

not x t f
  x f t

-- Church Constructors of Option a
none fst snd
  fst
some x fst snd
  snd x

-- TODO: Destructors of Option a
option(none: b, some: a: b, opt: option a): b
option onNone onSome fst snd


either: l r
"left": x l r: l x
"right": x l r: r x



