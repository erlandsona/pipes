identity l \l

const a b \a

flip f a b \f b a

(x y. x) 123

: [BadRequest] Person
x y
  \ x
   \ stuff : [Int] Str
   | Err int
   \ Sum.onClick
    \ y int
     \ things : String -> [BadRequest] Person




- nat -> nat -> Bool
check one two
  \one = two

- bool -> bool -> bool
and 1 2
  \1 & 2


- option a
| none : option a
| some : a -> option a
